
@{
    ViewBag.Title = "Lumendi";
}
<script src="~/assets/scripts/VASSurvey3.js?1015"></script>
<!-- BANNER
================================================== -->
<div class="banner banner--small" role="banner">
    <div class="banner__background" style="background-image:url(/assets/images/banner-contact.jpg);" data-type="background"></div>
    <div class="banner__content">
        <h2 class="banner__headline alpha">Lumendi</h2>
    </div>
</div><!--/banner -->


<main role="main" id="main">    
    <div class="section section--gray" style="padding: 40px 0">
        <div class="container">
            <p style="display: inline">Hi User <span id="UserID"></span>: Welcome to our study! Now, you can input your Subject.</p>
            <button class="btn" id="btnSignOut" style="float: right; transform: translateY(-20px)">Sign Out</button>
        </div>
    </div>
    <div class="container--fluid two-column__container">
        <form action="" id="Study3" name="LumendiForm">
            <div class="grid two-column">
                @*left*@
                <div class="grid__column grid__column--12 grid__column--2--md two-column__section two-column__section--blue" id="div-menu">
                    <div class="vertical-menu">
                        @*<a href="#" id="Visit0" targetLink="Visit0">Visit 1</a>
                            <a href="#" id="Visit1">Visit 2</a>
                            <a href="#" id="Visit2">Visit 3</a>
                            <a href="#" id="Visit3">Visit 4</a>
                            <a href="#" id="Visit4">Visit 5</a>*@

                        <button class="dropdown-btn" id="Visit0">Visit 1<i class="fa fa-caret-right"></i></button>
                        <div class="dropdown-container">
                            <a href="#" id="DemogVisit0">Demographics</a>
                            <a href="#" id="ProcVisit0">Procedure</a>
                        </div>
                        <button class="dropdown-btn" id="Visit1">Visit 2<i class="fa fa-caret-right"></i></button>
                        <div class="dropdown-container">
                            <a href="#" id="AEVisit1">Adverse Events</a>
                        </div>
                        <button class="dropdown-btn" id="Visit2">Visit 3<i class="fa fa-caret-right"></i></button>
                        <div class="dropdown-container">
                            <a href="#" id="AEVisit2">Adverse Events</a>
                        </div>
                        <button class="dropdown-btn" id="Visit3">Visit 4<i class="fa fa-caret-right"></i></button>
                        <div class="dropdown-container">
                            <a href="#" id="AEVisit3">Adverse Events</a>
                        </div>
                        <button class="dropdown-btn" id="Visit4">Visit 5<i class="fa fa-caret-right"></i></button>
                        <div class="dropdown-container">
                            <a href="#" id="AEVisit4">Adverse Events</a>
                        </div>

                        <a href="#" id="Summary">Summary</a>
                    </div>
                </div>
                @*right*@
                <div class="grid__column grid__column--12 grid__column--10--md two-column__section" id="div-Form">
                    @*Subject ID:*@
                    <div class="grid__column grid__column--12 grid__column--12--md" style="padding-bottom:50px">
                        <label for="SubjID" class="control__label control__label__inline control__label--is-required" id="lblSubjID">Subject ID: </label>
                        <input type="text" pattern="[0-9]{2}-[0-9]{3}" class="control__input__inline" style="width: 30%" id="SubjID" />
                    </div>
                    @*section Demog*@
                    <section class="grid" id="section-Demog">
                        <div class="grid__column grid__column--12 grid__column--12--md">
                            <h3>DEMOGRAPHICS</h3>
                        </div>
                        @*Demog left*@
                        <div class="grid__column grid__column--12 grid__column--6--md">
                            @*Age*@
                            <div class="grid__column grid__column--12 grid__column--12--md">

                                <label for="Demog-age" class="control__label control__label--is-required">Age</label>
                                <select class="control__select" id="Demog-age" style="display:inline">
                                    <option value="none" selected disabled hidden>
                                        --Select an Option--
                                    </option>
                                </select>
                                <p id="msg-Demog-age" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                            @*Gender*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="Demog-gender" class="control__label__inline control__label--is-required">Gender</label>
                                <fieldset>
                                    <label for="male">
                                        <input type="radio" id="male" value="Male" name="Demog-gender" /> Male
                                    </label>
                                    <label for="female">
                                        <input type="radio" id="female" value="Female" name="Demog-gender" /> Female
                                    </label>
                                </fieldset>
                                <p id="msg-Demog-gender" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                            @*div RaceEthni*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="Demog-RaceEthni" class="control__label control__label--is-required">Race/Ethnicity</label>
                                <fieldset>
                                    <input type="radio" id="Black" value="African American/Black/Not of Hispanic Origin" name="Demog-RaceEthni" />
                                    <label for="Black">
                                        African American/Black/Not of Hispanic Origin
                                    </label><br>

                                    <input type="radio" id="AmericanIndian" value="American Indian or Alaska Native" name="Demog-RaceEthni" />
                                    <label for="American Indian or Alaska Native">
                                        American Indian or Alaska Native
                                    </label><br>

                                    <input type="radio" id="Asian" value="Asian" name="Demog-RaceEthni" />
                                    <label for="Asian">
                                        Asian
                                    </label><br>

                                    <input type="radio" id="White" value="Caucasian/White/Not of Hispanic Origin" name="Demog-RaceEthni" />
                                    <label for="AmericanIndian">
                                        Caucasian/White/Not of Hispanic Origin
                                    </label><br>

                                    <input type="radio" id="Hispanic" value="Hispanic or Latino" name="Demog-RaceEthni" />
                                    <label for="Hispanic">
                                        Hispanic or Latino
                                    </label><br>

                                    <input type="radio" id="Hawaiian" value="Native Hawaiian or Other Pacific Islander" name="Demog-RaceEthni" />
                                    <label for="Hawaiian">
                                        Native Hawaiian or Other Pacific Islander
                                    </label><br>

                                    <input type="radio" id="Other" value="Other" name="Demog-RaceEthni" />
                                    <label for="Other">
                                        Other
                                    </label><br>
                                </fieldset>
                                <p id="msg-Demog-RaceEthni" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                        </div>

                        @*Demog right *@
                        <div class="grid__column grid__column--12 grid__column--6--md">
                            @*Previous Colon Polyp?*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="Demog-ColonPolyp" class="control__label__inline control__label--is-required">Previous Colon Polyp? </label>
                                <fieldset>
                                    <label for="ColonPolypYes">
                                        <input type="radio" id="ColonPolypYes" value="Yes" name="Demog-ColonPolyp" /> Yes
                                    </label>
                                    <label for="ColonPolypNo">
                                        <input type="radio" id="ColonPolypNo" value="No" name="Demog-ColonPolyp" /> No
                                    </label>
                                </fieldset>
                                <p id="msg-Demog-ColonPolyp" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                            @*Previous Biopsy Done?*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="Demog-BiopsyDone" class="control__label__inline control__label--is-required">Previous Biopsy Done? </label>
                                <fieldset>
                                    <label for="BiopsyDoneYes">
                                        <input type="radio" id="BiopsyDoneYes" value="Yes" name="Demog-BiopsyDone" /> Yes
                                    </label>
                                    <label for="BiopsyDoneNo">
                                        <input type="radio" id="BiopsyDoneNo" value="No" name="Demog-BiopsyDone" /> No
                                    </label>
                                </fieldset>
                                <p id="msg-Demog-ColonPolyp" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                            @*Hemorrhoids?*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="Demog-Hemorrhoids" class="control__label__inline control__label--is-required">Hemorrhoids? </label>
                                <fieldset>
                                    <label for="HemorrhoidsYes">
                                        <input type="radio" id="HemorrhoidsYes" value="Yes" name="Demog-Hemorrhoids" /> Yes
                                    </label>
                                    <label for="HemorrhoidsNo">
                                        <input type="radio" id="HemorrhoidsNo" value="No" name="Demog-Hemorrhoids" /> No
                                    </label>
                                </fieldset>
                                <p id="msg-Demog-Hemorrhoids" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                            @*History of Diverticulitis?*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="Demog-Diverticulitis" class="control__label__inline control__label--is-required">History of Diverticulitis? </label>
                                <fieldset>
                                    <label for="DiverticulitisYes">
                                        <input type="radio" id="DiverticulitisYes" value="Yes" name="Demog-Diverticulitis" /> Yes
                                    </label>
                                    <label for="DiverticulitisNo">
                                        <input type="radio" id="DiverticulitisNo" value="No" name="Demog-Diverticulitis" /> No
                                    </label>
                                </fieldset>
                                <p id="msg-Demog-Diverticulitis" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                            @*Diabetes? *@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="Demog-Diabetes" class="control__label__inline control__label--is-required">Diabetes?  </label>
                                <fieldset>
                                    <label for="DiabetesYes">
                                        <input type="radio" id="DiabetesYes" value="Yes" name="Demog-Diabete" /> Yes
                                    </label>
                                    <label for="DiabetesNo">
                                        <input type="radio" id="DiabetesNo" value="No" name="Demog-Diabetes" /> No
                                    </label>
                                </fieldset>
                                <p id="msg-Demog-Diverticulitis" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                            @*Anemia? *@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="Demog-Anemia" class="control__label__inline control__label--is-required">Anemia?  </label>
                                <fieldset>
                                    <label for="DiabetesYes">
                                        <input type="radio" id="AnemiaYes" value="Yes" name="Demog-Anemia" /> Yes
                                    </label>
                                    <label for="DiabetesNo">
                                        <input type="radio" id="AnemiaNo" value="No" name="Demog-Anemia" /> No
                                    </label>
                                </fieldset>
                                <p id="msg-Demog-Anemia" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                            @*Hysterectomy, C-Section, or Tubal Ligation? *@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="Demog-Hysterectomy" class="control__label__inline control__label--is-required">Hysterectomy, C-Section, or Tubal Ligation?  </label>
                                <fieldset>
                                    <label for="HysterectomyYes">
                                        <input type="radio" id="HysterectomyYes" value="Yes" name="Demog-Hysterectomy" /> Yes
                                    </label>
                                    <label for="HysterectomyNo">
                                        <input type="radio" id="HysterectomyNo" value="No" name="Demog-Hysterectomy" /> No
                                    </label>
                                </fieldset>
                                <p id="msg-Demog-Hysterectomy" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                            @*ASA Classification: *@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="Demog-ASA" class="control__label__inline control__label--is-required">ASA Classification:   </label>
                                <fieldset>
                                    <label for="ASA1">
                                        <input type="radio" id="ASA1" value="1" name="Demog-ASA" /> 1
                                    </label>
                                    <label for="ASA2">
                                        <input type="radio" id="ASA2" value="2" name="Demog-ASA" /> 2
                                    </label>
                                    <label for="ASA3">
                                        <input type="radio" id="ASA3" value="3" name="Demog-ASA" /> 3
                                    </label>
                                </fieldset>
                                <p id="msg-Demog-ASA" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                            @*Kudo Classification:  *@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="Demog-ASA" class="control__label__inline control__label--is-required">Kudo Classification:    </label>
                                <fieldset>
                                    <label for="Kudo1">
                                        <input type="radio" id="Kudo1" value="II" name="Demog-Kudo" /> II
                                    </label>
                                    <label for="Kudo2">
                                        <input type="radio" id="Kudo2" value="IIA" name="Demog-Kudo" /> IIA
                                    </label>
                                    <label for="Kudo3">
                                        <input type="radio" id="Kudo3" value="IIIL" name="Demog-Kudo" /> IIIL
                                    </label>
                                    <label for="Kudo4">
                                        <input type="radio" id="Kudo4" value="IIIS" name="Demog-Kudo" /> IIIS
                                    </label>
                                    <label for="Kudo5">
                                        <input type="radio" id="Kudo5" value="IV" name="Demog-Kudo" /> IV
                                    </label>
                                </fieldset>
                                <p id="msg-Demog-Kudo" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                            @*Paris Classification:  *@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="Demog-ASA" class="control__label__inline control__label--is-required">Paris Classification:     </label>
                                <fieldset>
                                    <label for="Paris1">
                                        <input type="radio" id="Paris1" value="IIa" name="Demog-Paris" /> IIa
                                    </label>
                                    <label for="Paris2">
                                        <input type="radio" id="Paris2" value="IIb" name="Demog-Paris" /> IIb
                                    </label>
                                    <label for="Paris3">
                                        <input type="radio" id="Paris3" value="IIa+IIb" name="Demog-Paris" /> IIa+IIb
                                    </label>
                                    <label for="Paris4">
                                        <input type="radio" id="Paris4" value="Ip" name="Demog-Paris" /> Ip
                                    </label>
                                    <label for="Paris5">
                                        <input type="radio" id="Paris5" value="Is" name="Demog-Paris" /> Is
                                    </label>
                                    <label for="Paris4">
                                        <input type="radio" id="Paris6" value="Is+IIa" name="Demog-Paris" /> Is+IIa
                                    </label>
                                    <label for="Paris5">
                                        <input type="radio" id="Paris7" value="Is+Ip" name="Demog-Paris" /> Is+Ip
                                    </label>
                                </fieldset>
                                <p id="msg-Demog-Paris" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                        </div>
                    </section>
                    @*section Procedure*@
                    <section class="grid" id="section-proc" style="display: none">
                        <div class="grid__column grid__column--12 grid__column--12--md">
                            <h3>PROCEDURE</h3>
                        </div>
                        @*Form Left*@
                        <div class="grid__column grid__column--12 grid__column--6--md">
                            @*Randomization*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="proc-Randomization" class="control__label control__label__inline control__label--is-required">Randomization Group: </label>
                                <fieldset>
                                    <label for="Device">
                                        <input type="radio" id="Device" value="Device" name="proc-Randomization" required /> Device
                                    </label>
                                    <label for="Control">
                                        <input type="radio" id="Control" value="Control" name="proc-Randomization" /> Control
                                    </label>
                                </fieldset>
                                <p id="msg-proc-Randomization" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>

                            @*Was Intervention with Device Successful? *@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="proc-Randomization" class="control__label control__label__inline control__label--is-required">If device used, was intervention with device successful?  </label>
                                <fieldset>
                                    <label for="DiviceSuccessfulYes">
                                        <input type="radio" id="DiviceSuccessfulYes" value="Yes" name="proc-DiviceSuccessful" required /> Yes
                                    </label>
                                    <label for="DiviceSuccessfulNo">
                                        <input type="radio" id="DiviceSuccessfulNo" value="No" name="proc-DiviceSuccessful" /> No
                                    </label>
                                </fieldset>
                                <p id="msg-proc-Randomization" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>

                            @*enter a freehand comment to describe why it was not successful.*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="proc-DescribeNotSuccess" class="control__label control__label control__label--is-required">If No, enter a freehand comment to describe why it was not successful.</label>
                                <textarea class="control__input" id="proc-DescribeNotSuccess"></textarea>
                                <p id="msg-proc-Length" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>

                            @*Length of Lesion:*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="proc-Length" class="control__label control__label__inline control__label--is-required">Length of Lesion:</label>
                                <input type="number" step="0.1" min="0" max="100" class="control__input__inline" id="proc-Length" required />cm
                                <p id="msg-proc-Length" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>

                            @*Width of Lesion:*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="proc-Width" class="control__label control__label__inline control__label--is-required">Width of Lesion:</label>
                                <input type="number" step="0.1" min="0" max="100" class="control__input__inline" id="proc-Width" required />cm
                                <p id="msg-proc-Width" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                        </div>

                        @*Form Right*@
                        <div class="grid__column grid__column--12 grid__column--6--md">
                            @*Lesion Location:*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="Demog-LesionLocation: " class="control__label control__label--is-required">Lesion Location: </label>
                                <fieldset>
                                    <label for="LesionLocation1">
                                        <input type="radio" id="LesionLocation1" value="Appendix" name="proc-LesionLocation" /> Appendix
                                    </label>
                                    <label for="LesionLocation2">
                                        <input type="radio" id="LesionLocation2" value="Ascending Colon" name="proc-LesionLocation" /> Ascending Colon
                                    </label>
                                    <label for="LesionLocation3">
                                        <input type="radio" id="LesionLocation3" value="Cecum" name="proc-LesionLocation" /> Cecum
                                    </label>
                                    <label for="LesionLocation4">
                                        <input type="radio" id="LesionLocation4" value="Descending Colon" name="proc-LesionLocation" /> Descending Colon
                                    </label><br>
                                    <label for="LesionLocation5">
                                        <input type="radio" id="LesionLocation5" value="Hepatic Flexure" name="proc-LesionLocation" /> Hepatic Flexure
                                    </label>
                                    <label for="LesionLocation6">
                                        <input type="radio" id="LesionLocation6" value="ICV" name="proc-LesionLocation" /> ICV
                                    </label>
                                    <label for="LesionLocation7">
                                        <input type="radio" id="LesionLocation7" value="Rectum" name="proc-LesionLocation" /> Rectum
                                    </label>
                                    <label for="LesionLocation8">
                                        <input type="radio" id="LesionLocation8" value="Sigmoid" name="proc-LesionLocation" /> Sigmoid
                                    </label><br>
                                    <label for="LesionLocation9">
                                        <input type="radio" id="LesionLocation9" value="Splenic Flexure" name="proc-LesionLocation" /> Splenic Flexure
                                    </label>
                                    <label for="LesionLocation10">
                                        <input type="radio" id="LesionLocation10" value="Transverse Colon" name="proc-LesionLocation" /> Transverse Colon
                                    </label>
                                </fieldset>
                                <p id="msg-proc-Width" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>
                            @*Navigating: *@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="proc-Randomization" class="control__label control__label control__label--is-required">Navigating: </label>
                                <fieldset>
                                    <label for="Navigating1">
                                        <input type="radio" id="Navigating1" value="Difficult" name="proc-Navigating" required /> Difficult
                                    </label>
                                    <label for="Navigating2">
                                        <input type="radio" id="Navigating2" value="Somewhat Difficult" name="proc-Navigating" /> Somewhat Difficult
                                    </label>
                                    <label for="Navigating3">
                                        <input type="radio" id="Navigating3" value="Somewhat Easy" name="proc-Navigating" required /> Somewhat Easy
                                    </label>
                                    <label for="Navigating4">
                                        <input type="radio" id="Navigating4" value="Easy" name="proc-Navigating" /> Easy
                                    </label>
                                </fieldset>
                                <p id="msg-proc-Navigating" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>

                            @*Clean Margins?*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label for="Demog-CleanMarginsYes" class="control__label__inline control__label--is-required">Clean Margins?</label>
                                <fieldset>
                                    <label for="CleanMarginsYes">
                                        <input type="radio" id="CleanMarginsYes" value="Yes" name="proc-CleanMargins" /> Yes
                                    </label>
                                    <label for="CleanMarginsNo">
                                        <input type="radio" id="CleanMarginsNo" value="No" name="proc-CleanMargins" /> No
                                    </label>
                                </fieldset>
                                <p id="msg-Demog-CleanMargins" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            </div>

                            @*Date of Procedure:*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label class="control__label control__label__inline control__label--is-required">Date of Procedure: </label>
                                <input type="date" class="control__input__inline" id="proc-DProc" required />
                            </div>
                            <p id="msg-proc-TBegan" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            @*Time Case Begin:*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label class="control__label control__label__inline control__label--is-required">Time Case Begin: </label>
                                <input type="time" class="control__input__inline" step="1" id="proc-TBegan" required />
                            </div>
                            <p id="msg-proc-TBegan" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            @*Time Case Ended:*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label class="control__label control__label__inline control__label--is-required">Time Case Ended: </label>
                                <input type="time" class="control__input__inline" step="1" id="proc-TEnded" required />
                            </div>
                            <p id="msg-proc-TEnded" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            @*Time when Ceum Reached:*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label class="control__label control__label__inline control__label--is-required">Time when Ceum Reached: </label>
                                <input type="time" class="control__input__inline" step="1" id="proc-TCeReached" required />
                            </div>
                            <p id="msg-proc-TCeReached" style="color:#ff0000; margin:0px; font-style:italic"></p>
                            @*Time when Lesion Reached:*@
                            <div class="grid__column grid__column--12 grid__column--12--md">
                                <label class="control__label control__label__inline control__label--is-required">Time when Lesion Reached: </label>
                                <input type="time" class="control__input__inline" step="1" id="proc-TLeReached" required />
                            </div>
                            <p id="msg-proc-TLeReached" style="color:#ff0000; margin:0px; font-style:italic"></p>
                        </div>

                    </section>
                    @*section ADVERSE EVENTS*@
                    <section class="grid" id="section-AE" style="display: none">
                        <div class="grid__column grid__column--12 grid__column--12--md">
