
@{
    ViewBag.Title = "Logecal Sitemap";
}
<!-- BANNER
================================================== -->
<div class="banner banner--small banner--gradient" role="banner">
    <div class="banner__content">
        <h1 class="banner__headline alpha">Sitemap</h1>
    </div>
</div><!--/banner -->
<main role="main" id="main">

    <!-- SITEMAP
    ================================================== -->
    <article class="section section--small">
        <div class="container container--medium">
            <div class="grid">
                <div class="grid__column grid__column--12 grid__column--6--sm">
                    <h2 class="h3">Site Links</h2>
                    <ul class="square-list">
                        <li class="square-list__item"><a href="/" class="text-link">Home</a></li>
                        <li class="square-list__item"><a href="/about" class="text-link">About</a></li>
                        <li class="square-list__item"><a href="/services" class="text-link">Services</a></li>
                        <li class="square-list__item"><a href="/contact" class="text-link">Contact</a></li>
                        <li class="square-list__item"><a href="/mailing-list" class="text-link">Mailing List</a></li>
                        <li class="square-list__item"><a href="/privacy-policy" class="text-link">Privacy Policy</a></li>
                        <li class="square-list__item"><a href="/terms-of-use" class="text-link">Terms of Use</a></li>
                    </ul>
                </div>
                <div class="grid__column grid__column--12 grid__column--6--sm">
                    <h2 class="h3">Contact Us</h2>
                    <address>
                        <ul class="address-list" aria-label="Contact Information">
                            <li class="address-list__item">
                                <div class="address-list__title">Address</div>
                                <div class="address-list__information">
                                    996 Old Eagle School Road<br />
                                    Suite 1106<br />
                                    Wayne, PA 19087
                                </div>
                            </li>
                            <li class="address-list__item">
                                <div class="address-list__title">Phone</div>
                                <div class="address-list__information">
                                    <a href="tel:14846541801" class="text-link">484.654.1801</a>
                                </div>
                            </li>
                            <li class="address-list__item">
                                <div class="address-list__title">Email</div>
                                <div class="address-list__information">
                                    <a href="mailto:info@logecal.com" class="text-link">info@logecal.com</a>
                                </div>
                            </li>
                            <li class="address-list__item address-list__item--center">
                                <div class="address-list__title">
                                    <a href="https://www.linkedin.com/company/logecal" target="_blank" class="social-icon"><i class="fa fa-linkedin"></i></a>
                                </div>
                                <div class="address-list__information">
                                    <a href="https://www.linkedin.com/company/logecal" target="_blank" class="text-link">Connect With Us</a>
                                </div>
                            </li>
                        </ul>
                    </address>
                </div>
            </div>
        </div>
    </article><!--/sitemap-->

</main>

