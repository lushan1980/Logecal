
@{
    ViewBag.Title = "navpagesample";
}

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1" />
    <title>@ViewBag.Title - Logecal Application</title>

    <link href="https://fonts.googleapis.com/css?family=Ubuntu:400,400i,500,700&display=swap" rel="stylesheet">
    <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">

    @Styles.Render("~/Assets/css")

</head>
<body>

    <!-- NAVBAR
        ================================================== -->
    <div class="navbar" style="background-color: #2684ba">
        <div class="container navbar__container" style="height:50px">
            <div class="navbar__left">
                <a href="/" class="navbar__logo">
                    <img class="navbar__logo-file navbar__logo-file--white" src="/assets/images/logo-white.png" alt="Logecal">
                </a>
            </div>
            <div class="navbar__mobile-buttons">
                <button class="navbar__toggle navicon" aria-label="Show Menu">
                    <span class="navicon__holder">
                        <span class="navicon__line"></span>
                        <span class="navicon__line"></span>
                        <span class="navicon__line"></span>
                    </span>
                </button>
            </div>
            <nav class="navbar__nav" style="margin-bottom:0">
                <ul class="nav navbar__nav-list">
                    <li class="nav__item"><a href="/Home/About" class="nav__link">About</a></li>
                    <li class="nav__item"><a href="/HOME/services" class="nav__link">Services</a></li>
                    <li class="nav__item"><a href="/VAS/SurveyList" class="nav__link">Your&nbsp;Study</a></li>
                    <li class="nav__item"><a href="/HOME/contact" class="nav__link">Contact</a></li>
                    <li class="nav__item nav__item--mobile-only">
                        <a href="https://www.linkedin.com/company/logecal" class="nav__link" aria-label="LinkedIn" target="_blank"><i class="fa fa-linkedin nav__link-icon"></i><span class="nav__link-label" aria-hidden="true">Follow Us On LinkedIn</span></a>
                    </li>
                </ul>
            </nav>
        </div>

        <nav class="fixed-nav" role="navigation" aria-hidden="true">
            <div class="container fixed-nav__container">
                <ul class="fixed-nav__nav">
                    <li class="fixed-nav__nav">
                        <a href="/" class="fixed-nav__link" tabIndex="-1">
                            <img class="fixed-nav__logo" src="/assets/images/logo-white.png" alt="">
                        </a>
                    </li>
                    <li class="fixed-nav__nav"><a href="/Home/About" class="fixed-nav__link" tabIndex="-1">About</a></li>
                    <li class="fixed-nav__nav"><a href="/Home/services" class="fixed-nav__link" tabIndex="-1">Services</a></li>
                    <li class="fixed-nav__nav"><a href="/COVID19/Index" class="fixed-nav__link" tabIndex="-1">COVID-19</a></li>
                    <li class="fixed-nav__nav"><a href="/VAS/SurveyList" class="fixed-nav__link" tabIndex="-1">Your&nbsp;Study</a></li>
                    <li class="fixed-nav__nav"><a href="/Home/contact" class="fixed-nav__link" tabIndex="-1">Contact</a></li>
                </ul>
            </div>
        </nav>
    </div><!--/navbar -->
